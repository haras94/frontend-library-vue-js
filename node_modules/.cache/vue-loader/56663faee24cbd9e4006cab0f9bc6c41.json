{\n    width: 400px;\n    height: auto;\n    background-color: #fff;\n    position: fixed;\n    top: 50%;\n    left: 50%;\n    transform: translate(-50%, -50%);\n    padding: 2em;\n    border-radius: 1em;\n    opacity: 0;\n    visibility: hidden;\n    transition: all .7s;\n}\n\n/* penampakan kotak modal add */\n#modaledit:target {\n    opacity: 1;\n    visibility: visible;\n}\n\n/* penampakan isi dalam modal add */\n#modaledit:target .modal_contentedit {\n    opacity: 1;\n    visibility: visible;\n}\n\n/* tanda silang close dalam modal add */\n.edita {\n    color: #ee0909;\n    position: absolute;\n    width: 30px;\n    height: 30px;\n    right: 3px;\n    top: 7px;\n    font-weight: bolder;\n    font-size: 24px;\n\n}\n\n/* tulisan add */\n.modal-openedit {\n    position: absolute;\n    width: 244px;\n    height: 56px;\n    left: -3px;\n    top: 480px;\n    font-family: 'Montserrat', sans-serif;\n    font-style: normal;\n    font-weight: 600;\n    font-size: 25px;\n    line-height: 34px;\n    color: #000000;\n}\n\n/* header isi dalam modal */\n.modal_isiedit {\n    position: absolute;\n    width: 279px;\n    height: 66px;\n    left: 169px;\n    top: 109px;\n    font-family: 'Montserrat', sans-serif;\n}\n\n/* input class */\n.baris {\n    top: 1800px;\n    display: flex;\n    align-items: center;\n\n}\n\n/* */\n.kolom-40 {\n    width: 20%;\n}\n\n.kolom-60 {\n    width: 80%;\n}\n\n/* header */\n.judul {\n    top: 1000px;\n    font-family: 'Montserrat', sans-serif;\n\n}\n\nh2.judul-edit {\n    margin-top: 0px;\n    font-family: 'Montserrat', sans-serif;\n}\n\n/* induk form */\n#forminput input,\n#forminput textarea {\n    width: 100%;\n    padding: 8px;\n    box-sizing: border-box;\n}\n\n/* title */\n.title {\n    margin-top: 10px;\n    font-family: 'Montserrat', sans-serif;\n}\n\n/* description */\n.penjelasan {\n    margin-top: 10px;\n    font-family: 'Montserrat', sans-serif;\n}\n\n/* tombol save */\n.submit {\n    margin-top: 20px;\n    width: 110px;\n    height: 30