div>\n\n          <div class=\"penjelasan baris\">\n            <div class=\"kolom-40\">\n              <label for=\"description\">Description</label></div>\n              <div class=\"kolom-60\">\n                <textarea class=\"description\" name=\"descripion\" id=\"\"  rows=\"5\"\n                placeholder=\"Lorem ipsum\n                dolor sit amet consectetur adipisicing elit. Eos nam eveniet corrupti\n                itaque totam doloremque, molestias et praesentium ipsum accusantium?\"></textarea>\n              </div>\n          </div>\n          <div class=\"submit\">\n            <button type=\"submit\">SAVE</button>\n          </div>\n      </form>\n      </div>\n      </div>\n      </div>\n    </div>\n    </div>\n</template>\n\n<script>\nimport Axios from 'axios';\n\nexport default {\n  data() {\n    return {\n      idBook: '',\n    };\n  },\n  methods: {\n    isinya() {\n      Axios\n        .get(`http://localhost:3500/api/v1/book${this.$route.params.id}`).then((getisinya) => {\n          this.idBook = getisinya.data.result;\n          document.querySelector('.coveratas').style.coveratas = `url(${this.idBook.image})`;\n          document.querySelector('.coverkecil').style.coverkecil = `url(${this.idBook.image})`;\n        });\n    },\n  },\n\n  created() {\n    this.isinya();\n  },\n};\n</script>\n\n<style lang=\"css\" scoped>\n    .backbutton svg{\n    width: 20px;\n    transform: rotate(-90deg);\n    margin-top: 13px;\n    display: block;\n    margin-left: 15px;\n}\n\n.backbutton{\n    position: absolute;\n    z-index: 1000;\n    top: 15px;\n    left: 10px;\n    background-color: white;\n    width: 50px;\n    height: 50px;\n    border-radius: 50%;\n    box-shadow: 0 5px 6px rgba(0, 0, 0, 0.2);\n}\n\n.coveratas{\n    position: absolute;\n    width: 100vw;\n    height: 454px;\n    left: -2px;\n    top: 0px;\n}\n\n.coverbukukecil{\n    position: absolute;\n    width: 200px;\n    height: 288.81px;\n    right: 50px;\n    top: 253px;\n    border-radiu