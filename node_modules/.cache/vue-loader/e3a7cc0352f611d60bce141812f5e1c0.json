\n.tulisanlbr {\n    /* position: absolute; */\n    width: auto;\n    margin-top: 17px;\n    font-weight: bold;\n    font-size: 20px;\n    line-height: 22px;\n    font-family: 'Montserrat', sans-serif;\n    margin-left: 5px;\n}\n\n.icnlbr {\n    display: flex;\n    flex-wrap: nowrap;\n}\n\n/* menu pada navbar */\n\n/* tulisan home */\n.homee {\n    /* position: absolute; */\n    width: auto;\n    font-family: 'Montserrat', sans-serif;\n    font-style: normal;\n    font-weight: 600;\n    font-size: 17px;\n    line-height: 22px;\n    color: #000000;\n    font-weight: bold;\n}\n\n.home {\n    /* position: absolute; */\n    width: auto;\n    font-size: 17px;\n    line-height: 22px;\n    font-weight: bolder;\n    font-family: 'Montserrat', sans-serif;\n    color: #000000;\n    display: flex;\n    flex-wrap: nowrap;\n    align-items: center;\n    margin-left: 20px;\n}\n\n.time {\n    /* position: absolute; */\n    width: auto;\n    font-size: 17px;\n    line-height: 22px;\n    font-weight: bolder;\n    font-family: 'Montserrat', sans-serif;\n    color: #000000;\n    display: flex;\n    flex-wrap: nowrap;\n    align-items: center;\n    margin-left: 20px;\n\n}\n\n.cari {\n    display: flex;\n    flex-wrap: nowrap;\n    align-items: center;\n}\n\n.panahbawah {\n    width: 10px;\n    height: 8px;\n    margin-left: 5px;\n}\n\n.panahbawah1 {\n    width: 10px;\n    height: 8px;\n    margin-left: 5px;\n}\n\n/* kolom pencarian */\n.search {\n    width: 400px;\n    height: 43px;\n    left: 723px;\n    top: 10px;\n    padding-left: 50px;\n    background: #FFFFFF;\n    border: 1px solid #CACACA;\n    box-sizing: border-box;\n    border-radius: 20px;\n    margin-right: 130px;\n\n\n}\n\n.glass {\n    width: 20px;\n    height: 20px;\n    margin-right: -38px;\n    position: relative;\n    z-index: 1;\n}\n\n/* sidebar */\n.sidebar {\n    height: 100%;\n    width: 200px;\n    position: absolute;\n    z-index: 1;\n    top: 0;\n    left: -80px;\n    background: #FFFFFF;\n    box-shadow: 0px 4px 30px rgba(0, 0, 0, 0.25);\n    transition: 0.8s;\n    margin-left: -200px;\n}\n\n.sidebarb {\n    height: 100%;\n    width: 200px;\n    position: absolute;\n    z-index: 1;\n    top: 0;\n    left: 0px;\n    background: #FFFFFF;\n    box-shadow: 0px 4px 30px rgba(0, 0, 0, 0.25);\n    margin-left: 0px;\n    /*agar sidebar tidak muncul duluan*/\n    transition: 0.8s;\n}\n\n.sidebar a {\n    padding: 6px 8px 6px 16px;\n    text-decoration: none;\n    font-size: 20px;\n    color: black;\n    display: block;\n}\n\n/* .sidebar a:hover {\n    color: #f1f1f1;\n  } */\n\n/* menu explore */\n.explore {\n    position: absolute;\n    width: 244px;\n    height: 56px;\n    left: -3px;\n    top: 345px;\n    font-family: 'Montserrat', sans-serif;\n    font-style: normal;\n    font-weight: 600;\n    font-size: 25px;\n    line-height: 34px;\n    color: #000000;\n}\n\n/* menu history */\n.history {\n    position: absolute;\n    width: 244px;\n    height: 56px;\n    left: -3px;\n    top: 415px;\n\n    font-family: 'Montserrat', sans-serif;\n    font-style: normal;\n    font-weight: 600;\n    font-size: 25px;\n    line-height: 34px;\n    color: #000000;\n}\n\n/* menu add book */\n.add-book {\n    position: absolute;\n    width: 148px;\n    height: 33px;\n    left: -3px;\n    top: 486px;\n\n    font-family: 'Montserrat', sans-serif;\n    font-style: normal;\n    font-weight: 600;\n    font-size: 25px;\n    line-height: 34px;\n    color: #000000;\n}\n\n/* foto profile */\n.fotoniki {\n    position: absolute;\n    width: 150px;\n    height: 150px;\n    left: 20px;\n    top: 66px;\n\n    border-radius: 150px;\n}\n\n/* nama niki */\n.namaniki {\n    position: absolute;\n    width: 244px;\n    height: 78px;\n    left: 13px;\n    top: 232px;\n\n    font-size: 30px;\n    line-height: 39px;\n    color: #000000;\n}\n\n/* Float coloums */\n.column {\n    box-sizing: border-box;\n    padding: 0px 20px;\n    flex-wrap: nowrap;\n    display: flex;\n    justify-content: flex-end;\n    background: #FFFFFF;\n    margin-left: 170x;\n    margin-bottom: 150px;\n\n}\n\n/* menghilangkan margin sisa kanan kiri */\n.row {\n    display: flex;\n    flex-wrap: wrap;\n    margin-top: 280px;\n    justify-content: center;\n}\n\n\n/* menghilangkan float pada coloum */\n/* .row:after {\n    content: \"\";\n    display: table;\n    clear: both;\n    } */\n\n/* style pada card */\n.card {\n    width: 300px;\n    height: 180px;\n    left: 379px;\n    top: 574px;\n    text-align: center;\n    padding: \"16px\";\n    box-shadow: 0px 4px 25px rgba(0, 0, 0, 0.25);\n    border-radius: 15px;\n}\n\n\n.listbook {\n    position: absolute;\n    width: 242px;\n    height: 43px;\n    left: 120px;\n    top: 515px;\n    font-size: 25px;\n    line-height: 33px;\n    color: #424242;\n    font-family: 'Montserrat', sans-serif;\n}\n\na {\n    text-decoration: none;\n    color: black;\n}\n\na:hover {\n    color: grey;\n\n}\n\n/* titik 3 pada sidebar */\n\n\n/* Modal dalam sidebar add data */\n\n/* text dekorasi pada link modal add */\n.edita:link,\n.edita:visited {\n    text-decoration: none;\n}\n\n/* kontainer modal add */\n.modaledit {\n    width: 1340px;\n    height: 454px;\n    position: fixed;\n    top: 0px;\n    left: 0;\n    z-index: 10;\n    opacity: 0;\n    visibility: hidden;\n    transition: all .8s;\n}\n\n/* kotak modal add */\n.modal_contentedit {\n    width: 400px;\n    height: auto;\n    background-color: #fff;\n    position: fixed;\n    top: 50%;\n    left: 50%;\n    transform: translate(-50%, -50%);\n    padding: 2em;\n    border-radius: 1em;\n    opacity: 0;\n    visibility: hidden;\n    transition: all .7s;\n}\n\n/* penampakan kotak modal add */\n#modaledit:target {\n    opacity: 1;\n    visibility: visible;\n}\n\n/* penampakan isi dalam modal add */\n#modaledit:target .modal_contentedit {\n    opacity: 1;\n    visibility: visible;\n}\n\n/* tanda silang close dalam modal add */\n.edita {\n    color: #ee0909;\n    position: absolute;\n    width: 30px;\n    height: 30px;\n    right: 3px;\n    top: 7px;\n    font-weight: bolder;\n    font-size: 24px;\n\n}\n\n/* tulisan add */\n.modal-openedit {\n    position: absolute;\n    width: 244px;\n    height: 56px;\n    left: -3px;\n    top: 480px;\n    font-family: 'Montserrat', sans-serif;\n    font-style: normal;\n    font-weight: 600;\n    font-size: 25px;\n    line-height: 34px;\n    color: #000000;\n\n}\n\n/* header isi dalam modal */\n.modal_isiedit {\n    position: absolute;\n    width: 279px;\n    height: 66px;\n    left: 169px;\n    top: 109px;\n    font-family: 'Montserrat', sans-serif;\n}\n\n/* input class */\n.baris {\n    top: 1800px;\n    display: flex;\n    align-items: center;\n\n}\n\n/* */\n.kolom-40 {\n    width: 20%;\n}\n\n.kolom-60 {\n    width: 80%;\n}\n\n/* header */\n.judul {\n    top: 1000px;\n    font-family: 'Montserrat', sans-serif;\n\n}\n\nh2.judul-edit {\n    margin-top: 0px;\n    font-family: 'Montserrat', sans-serif;\n}\n\n\n/* induk form */\n#forminput input,\n#forminput textarea {\n    width: 100%;\n    padding: 8px;\n    box-sizing: border-box;\n}\n\n\n/* title */\n.title {\n    margin-top: 10px;\n    font-family: 'Montserrat', sans-serif;\n}\n\n/* description */\n.penjelasan {\n    margin-top: 10px;\n    font-family: 'Montserrat', sans-serif;\n}\n\n/* tombol save */\n.submit {\n    margin-top: 20px;\n    width: 110px;\n    height: 30px;\n    margin-left: auto;\n    font-family: 'Montserrat', sans-serif;\n}\n\n.submit button {\n    width: 100%;\n    height: 100%;\n    background: #F4CF5D;\n    color: white;\n    border: none;\n    border-radius: 5px;\n}\n\n/* caraousel button */\n.buttonprev {\n    position: absolute;\n    z-index: 10;\n    top: 200px;\n    right: 300px;\n    background-color: blue;\n    width: 50px;\n    height: 50px;\n    border-radius: 50%;\n    /* box-shadow: 0 5px 6px rgba(0, 0, 0, 0.2); */\n}\n\n.buttonnext {\n    position: absolute;\n    z-index: 10;\n    top: 200px;\n    left: 300px;\n    background-color: blue;\n    width: 50px;\n    height: 50px;\n    border-radius: 50%;\n    /* box-shadow: 0 5px 6px rgba(0, 0, 0, 0.2); */\n}\n\n/* caraousel isi */\n.caraousel {\n    flex-wrap: nowrap !important;\n    overflow: hidden;\n    display: flex;\n    width: 100%;\n    justify-content: center;\n    margin-top: 50px;\n\n\n}\n\n.car1 {\n    /* position: absolute; */\n    width: auto;\n    height: 200px;\n    /* top: 120px; */\n    border-radius: 15px;\n    display: block;\n    margin-right: 30px;\n}\n\n.car2 {\n    /* position: absolute; */\n    width: auto;\n    height: 200px;\n    /* top: 116px; */\n    border-radius: 15px;\n    display: block;\n    margin-right: 30px;\n}\n\n.car3 {\n    /* position: absolute; */\n    width: auto;\n    height: 200px;\n    /* top: 130px; */\n    border-radius: 15px;\n    display: block;\n    margin-right: 30px;\n}\n</style>\n"]}]}